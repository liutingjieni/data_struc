le(getline(in, line)) {
        int i = 0;
    while (line[i]) {
                count++;
                n++;
                cout << line[i];
                auto ret = word_count.insert({line[i], 1});
        if (!ret.second) {
                        ++ret.first->second;
                        count--;
                    
        }
                i++;
                
    }
        
}

